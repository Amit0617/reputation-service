{
  "name": "interrep-reputation-service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prepare": "cd contracts && yarn && yarn compile",
    "build": "NODE_OPTIONS='--max-old-space-size=1536' next build --debug",
    "analyze": "ANALYZE=true yarn build",
    "start": "next start",
    "script": "node -r dotenv/config -r tsconfig-paths/register -r ts-node/register",
    "seed:zeroHashes": "npm run script -- scripts/seeding/seedZeroHashes.ts",
    "seed:groups": "npm run script -- scripts/seeding/seedGroups.ts",
    "seed:twitterUsers": "npm run script -- scripts/seeding/seedTwitterUsers.ts",
    "test": "jest",
    "test:w": "jest --watch",
    "test:tree": "npx jest src/controllers/MerkleTreeController.test.ts --coverage false",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "commit": "cz",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  },
  "cacheDirectories": [
    "node_modules/",
    ".next/cache/"
  ],
  "dependencies": {
    "@fontsource/roboto": "^4.5.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@sentry/nextjs": "^6.7.1",
    "big-integer": "^1.6.48",
    "bnc-onboard": "^1.34.1",
    "botometer": "^1.0.12",
    "circomlib": "^0.5.3",
    "dotenv": "^8.2.0",
    "eth-sig-util": "^3.0.1",
    "ethers": "^5.4.5",
    "hardhat": "^2.3.0",
    "mongoose": "^5.12.1",
    "next": "10.0.9",
    "next-auth": "^3.14.6",
    "react": "17.0.1",
    "react-device-detect": "^1.17.0",
    "react-dom": "17.0.1",
    "semethid": "^0.5.0",
    "ts-node": "^10.0.0",
    "twitter-v2": "^1.0.8",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@next/bundle-analyzer": "^10.2.0",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/jest": "^26.0.22",
    "@types/next-auth": "^3.13.0",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "babel-jest": "^26.6.3",
    "chai": "^4.2.0",
    "colors": "^1.4.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.1.2",
    "mongodb-memory-server": "^7.3.6",
    "msw": "^0.29.0",
    "node-mocks-http": "^1.10.1",
    "prettier": "^2.2.1",
    "prettier-plugin-solidity": "^1.0.0-beta.10",
    "ts-jest": "^26.5.4",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
